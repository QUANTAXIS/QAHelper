{
	"name": "qahelper",
	"displayName": "QAHelper",
	"publisher": "somewheve",
	"author": {"email": "somewheve@gmail.com", "name": "somewheve", "url": "https://github.com/somewheve"},
	"description": "Helper of QAManual and generate annotation in Python Function",
	"version": "0.0.3",
	"engines": {
		"vscode": "^1.42.0"
	},
	"repository": {"url": "https://github.com/QUANTAXIS/QAHelper"},
	"categories": [
		"Other"
	],

	"activationEvents": [
		"onCommand:extension.GenerateAnnotation"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.GenerateAnnotation",
				"title": "QAFastGenerate"
			}
		],
		"menus": {
			"editor/context": [
				{
					"when": "editorFocus",
					"command": "extension.GenerateAnnotation",
					"group": "navigation"
				}
			]
		},
		"keybindings": [
			{
				"command": "extension.GenerateAnnotation",
				"key": "ctrl+alt+i",
				"when": "editorTextFocus"
			}
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.1",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.42.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.0.1",
		"typescript": "^3.7.5",
		"vscode-test": "^1.3.0"
	}
}
